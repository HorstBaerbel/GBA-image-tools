    "version": "0.2.0",
    "configurations": [
        {
            "name": "img2h debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${command:cmake.launchTargetPath}",
            "args": [
                "--blackwhite=0.7",
                "--outformat=bgr555",
                "--tiles",
                "--prune=1",
                "\"${workspaceFolder}/data/images/240x160/BigBuckBunny_40_240x160.png\"",
                "image"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Automatische Strukturierung und Einrückung für \"gdb\" aktivieren",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Do not step into library code",
                    "text": "-interpreter-exec console \"skip -gfi /usr/include/c++/*/*/*\nskip -gfi /usr/include/c++/*/*\nskip -gfi /usr/include/c++/*\"",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "vid2h debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${command:cmake.launchTargetPath}",
            "args": [
                "--dryrun",
                "--truecolor=rgb888",
                "--outformat=bgr555",
                "--dxtv=97",
                //"--lz10",
                "\"/home/raer/SynologyDrive/Programmieren/GameBoy/TAGABOW Singles/data/AMERICAN FOOD render 1 240x160.mp4\"",
                //"\"${workspaceFolder}/data/videos/240x160/BigBuckBunny_240x160_15fps.mp4\"",
                "video"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Automatische Strukturierung und Einrückung für \"gdb\" aktivieren",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Do not step into library code",
                    "text": "-interpreter-exec console \"skip -gfi /usr/include/c++/*/*/*\nskip -gfi /usr/include/c++/*/*\nskip -gfi /usr/include/c++/*\"",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "vid2h perf",
            "type": "cppdbg",
            "request": "launch",
            "program": "/usr/bin/perf",
            "args": [
                "record",
                "${command:cmake.launchTargetPath}",
                "--truecolor=rgb888",
                "--outformat=bgr555",
                "--dxtv=95",
                "--lz10",
                "\"${workspaceFolder}/data/videos/240x160/BigBuckBunny_240x160_15fps.mp4\"",
                "video"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": []
        },
        {
            "name": "vid2hplay debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${command:cmake.launchTargetPath}",
            "args": [
                "video.bin"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Automatische Strukturierung und Einrückung für \"gdb\" aktivieren",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Do not step into library code",
                    "text": "-interpreter-exec console \"skip -gfi /usr/include/c++/*/*/*\nskip -gfi /usr/include/c++/*/*\nskip -gfi /usr/include/c++/*\"",
                    "ignoreFailures": true
                }
            ]
        },
        {
