    "version": "0.2.0",
    "configurations": [
        {
            "name": "img2h debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${command:cmake.launchTargetPath}",
            "args": [
                "--shift=128",
                "--tilemap",
                "--movecolor0=00ff00",
                "\"${workspaceFolder}/../Remute-Unity/Gfx_work/slides/glow1.png\"",
                "bla"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Automatische Strukturierung und Einr端ckung f端r \"gdb\" aktivieren",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "vid2h debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${command:cmake.launchTargetPath}",
            "args": [
                "--truecolor=RGB565",
                "--dxt1",
                "--rle",
                "--lz10",
                "--vram",
                "${workspaceFolder}/test_data/videos/bbb_160x128_24fps_part.mp4",
                "video"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Automatische Strukturierung und Einr端ckung f端r \"gdb\" aktivieren",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}